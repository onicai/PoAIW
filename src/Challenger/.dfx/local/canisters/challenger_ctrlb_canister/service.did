type ApiError = variant {
  FailedOperation;
  InvalidId;
  ZeroAddress;
  Unauthorized;
  StatusCode : StatusCode;
  Other : text;
};
type AuthRecord = record { auth : text };
type AuthRecordResult = variant { Ok : AuthRecord; Err : ApiError };
type CanisterIDRecord = record { canister_id : text };
type CanisterIDRecordResult = variant { Ok : CanisterIDRecord; Err : ApiError };
type ChallengerCtrlbCanister = service {
  add_llm_canister_id : (CanisterIDRecord) -> (StatusCodeRecordResult);
  amiController : () -> (StatusCodeRecordResult) query;
  checkAccessToLLMs : () -> (StatusCodeRecordResult);
  generateNewChallenge : () -> (GeneratedChallengeResult);
  getGameStateCanisterId : () -> (text) query;
  getRoundRobinCanister : () -> (CanisterIDRecordResult) query;
  health : () -> (StatusCodeRecordResult) query;
  ready : () -> (StatusCodeRecordResult);
  setGameStateCanisterId : (text) -> (AuthRecordResult);
  setRoundRobinLLMs : (nat) -> (StatusCodeRecordResult);
  set_llm_canister_id : (CanisterIDRecord) -> (StatusCodeRecordResult);
  whoami : () -> (principal) query;
};
type GeneratedChallenge = record {
  generatedByLlmId : text;
  generationPrompt : text;
  generationId : text;
  generatedTimestamp : nat64;
  generatedChallengeText : text;
};
type GeneratedChallengeResult = variant {
  Ok : GeneratedChallenge;
  Err : ApiError;
};
type StatusCode = nat16;
type StatusCodeRecord = record { status_code : nat16 };
type StatusCodeRecordResult = variant { Ok : StatusCodeRecord; Err : ApiError };
service : ChallengerCtrlbCanister