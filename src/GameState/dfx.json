{
  "version": 1,
  "canisters": {
    "game_state_canister": {
      "main": "src/Main.mo",
      "type": "motoko",
      "dependencies": [
        "icp_ledger_canister",
        "cycles_minting_canister"
      ]
    },
    "icp_ledger_canister": {
      "type": "pull",
      "id": "ryjl3-tyaaa-aaaaa-aaaba-cai",
      "init_arg": "(variant { Init = record { minting_account = \"$(dfx identity get-principal)\"; initial_values = vec {}; send_whitelist = vec {};      transfer_fee = opt record { e8s = 10_000 : nat64; }; token_symbol = opt \"LICP\"; token_name = opt \"Local ICP\"; } })"
    },
    "cycles_minting_canister": {
      "type": "pull",
      "id": "rkp4c-7iaaa-aaaaa-aaaca-cai"
    }
  },
  "output_env_file": ".env",
  "defaults": {
    "build": {
      "packtool": "mops sources"
    }
  },
  "networks": {
    "development": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "testing": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "backup": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    }
  }
}