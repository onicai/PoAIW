type ApiError = variant {
  FailedOperation;
  InvalidId;
  ZeroAddress;
  Unauthorized;
  StatusCode : StatusCode;
  Other : text;
};
type AuthRecord = record { auth : text };
type AuthRecordResult = variant { Ok : AuthRecord; Err : ApiError };
type CanisterAddress = text;
type CanisterInput = record {
  canisterType : ProtocolCanisterType;
  address : CanisterAddress;
};
type CanisterRetrieveInput = record { address : CanisterAddress };
type Challenge = record {
  status : ChallengeStatus;
  challengePrompt : text;
  creationTimestamp : nat64;
  createdBy : CanisterAddress;
  responsibleJudgeAddress : CanisterAddress;
  challengeId : text;
  closedTimestamp : opt nat64;
};
type ChallengeAdditionResult = variant { Ok : Challenge; Err : ApiError };
type ChallengeParticipantEntry = record {
  result : ChallengeParticipationResult;
  reward : ChallengeWinnerReward;
  ownedBy : principal;
  submittedBy : principal;
  submissionId : text;
};
type ChallengeParticipationResult = variant {
  ThirdPlace;
  SecondPlace;
  Winner;
  Other : text;
  Participated;
};
type ChallengeResponseSubmissionInput = record {
  submittedBy : principal;
  challengeId : text;
  response : text;
};
type ChallengeResponseSubmissionResult = variant {
  Ok : ChallengeResponseSubmissionReturn;
  Err : ApiError;
};
type ChallengeResponseSubmissionReturn = record {
  status : ChallengeResponseSubmissionStatus;
  submittedTimestamp : nat64;
  success : bool;
  submissionId : text;
};
type ChallengeResponseSubmissionStatus = variant {
  Judged;
  FailedSubmission;
  Processed;
  Received;
  Other : text;
  Submitted;
};
type ChallengeResult = variant { Ok : Challenge; Err : ApiError };
type ChallengeStatus = variant { Open; Closed; Archived; Other : text };
type ChallengeWinnerDeclaration = record {
  participants : List;
  thirdPlace : ChallengeParticipantEntry;
  winner : ChallengeParticipantEntry;
  secondPlace : ChallengeParticipantEntry;
  finalizedTimestamp : nat64;
  challengeId : text;
};
type ChallengeWinnerReward = record {
  distributed : bool;
  rewardDetails : text;
  rewardType : RewardType;
  amount : nat;
  distributedTimestamp : opt nat64;
};
type ChallengeWinnersResult = variant {
  Ok : vec ChallengeWinnerDeclaration;
  Err : ApiError;
};
type ChallengesResult = variant { Ok : vec Challenge; Err : ApiError };
type GameStateCanister = service {
  addChallenge : (NewChallengeInput) -> (ChallengeAdditionResult);
  addMainerAgentCanister : (MainerAgentCanisterInput) -> (
      MainerAgentCanisterResult,
    );
  addOfficialCanister : (CanisterInput) -> (AuthRecordResult);
  addScoredResponse : (ScoredResponseInput) -> (ScoredResponseResult);
  createUserMainerAgentCanister : (MainerConfigurationInput) -> (
      MainerAgentCanisterResult,
    );
  getCurrentChallenges : () -> (ChallengesResult) query;
  getCurrentChallengesAdmin : () -> (ChallengesResult) query;
  getMainerAgentCanisterInfo : (CanisterRetrieveInput) -> (
      MainerAgentCanisterResult,
    ) query;
  getOfficialChallengerCanisters : () -> (AuthRecordResult);
  getRandomOpenChallenge : () -> (ChallengeResult);
  getRecentChallengeWinners : () -> (ChallengeWinnersResult) query;
  submitChallengeResponse : (ChallengeResponseSubmissionInput) -> (
      ChallengeResponseSubmissionResult,
    );
};
type List = opt record { ChallengeParticipantEntry; List };
type MainerAgentCanisterInput = record {
  canisterType : ProtocolCanisterType;
  ownedBy : principal;
  address : CanisterAddress;
};
type MainerAgentCanisterResult = variant {
  Ok : OfficialProtocolCanister;
  Err : ApiError;
};
type MainerConfigurationInput = record { aiModel : opt SelectableMainerLLM };
type NewChallengeInput = record { challengePrompt : text };
type OfficialProtocolCanister = record {
  canisterType : ProtocolCanisterType;
  ownedBy : principal;
  creationTimestamp : nat64;
  createdBy : principal;
  address : CanisterAddress;
};
type ProtocolCanisterType = variant {
  MainerAgent;
  Challenger;
  Judge;
  Verifier;
  MainerCreator;
};
type RewardType = variant {
  ICP;
  Coupon : text;
  MainerToken;
  Cycles;
  Other : text;
};
type ScoredResponseInput = record {
  status : ChallengeResponseSubmissionStatus;
  judgedBy : principal;
  submittedTimestamp : nat64;
  submittedBy : principal;
  score : nat;
  challengeId : text;
  response : text;
  submissionId : text;
};
type ScoredResponseResult = variant {
  Ok : ScoredResponseReturn;
  Err : ApiError;
};
type ScoredResponseReturn = record { success : bool };
type SelectableMainerLLM = variant { Qwen2_5_0_5_B };
type StatusCode = nat16;
service : GameStateCanister